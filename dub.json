{
	"name": "ebbcfd",
	"description": "A finite volume euler equation cfd solver",
	"copyright": "Copyright Â© 2016, Robert F. Rau II",
	"authors": ["Robert F. Rau II"],
	"dependencies": {
		"numd": "~master"
	},
	"configurations": [
		{
			"excludedSourceFiles": [
				"source/solver.d",
				"source/plotter.d",
				"source/post.d"
			],
			"name": "mesher",
			"targetName": "ebb-mesh",
			"targetType": "executable"
		},
		{
			"excludedSourceFiles": [
				"source/mesher.d",
				"source/plotter.d",
				"source/post.d"
			],
			"name": "solver",
			"targetName": "ebb-solve",
			"targetType": "executable"
		},
		{
			"excludedSourceFiles": [
				"source/mesher.d",
				"source/solver.d",
				"source/post.d"
			],
			"name": "plotter",
			"targetName": "ebb-plot",
			"targetType": "executable"
		},
		{
			"excludedSourceFiles": [
				"source/mesher.d",
				"source/solver.d",
				"source/plotter.d"
			],
			"name": "post",
			"targetName": "ebb-post",
			"targetType": "executable"
		}
	],
	"buildTypes": {
		"debug": {
			"buildOptions": ["debugMode", "debugInfo"]
		},
		"ldc-release": {
			"buildOptions": ["releaseMode", "inline"],
			"dflags": ["-O5"]
		},
		"flux-release": {
			"buildOptions": ["releaseMode", "inline"],
			"dflags": ["-O5"],
			"lflags": ["-L/usr/lib64", "-L~/ldc2-1.0.0-linux-x86_64/lib"]
		},
		"ldc-release-prof": {
			"buildOptions": ["releaseMode", "inline", "debugInfo"],
			"dflags": ["-O5"]
		}
	}
}
